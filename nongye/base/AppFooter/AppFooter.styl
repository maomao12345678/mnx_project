.appfooter
	height 100rpx
	width 100%
	background-color #FFFFFF
	border-top 1px solid #d2d2d2
	position fixed
	bottom 0
	left 0
	color #555
	font-size 24rpx
	.appfooter-item
		width 50%
		height 100%
		align-items center
		flex-direction column
		&.active
			color var(--color-theme)
		>.iconfont
			font-size 40rpx
			margin-bottom 5rpx